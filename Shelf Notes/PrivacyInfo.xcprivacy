<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- App Tracking Transparency: Shelf Notes does not track users across apps/websites. -->
	<key>NSPrivacyTracking</key>
	<false/>
	<key>NSPrivacyTrackingDomains</key>
	<array/>

	<!-- Data collection: This manifest intentionally declares no collected data types. -->
	<key>NSPrivacyCollectedDataTypes</key>
	<array/>

	<!-- Required Reason API usage (ITMS-91053). -->
	<key>NSPrivacyAccessedAPITypes</key>
	<array>
		<dict>
			<key>NSPrivacyAccessedAPIType</key>
			<string>NSPrivacyAccessedAPICategoryUserDefaults</string>
			<key>NSPrivacyAccessedAPITypeReasons</key>
			<array>
				<!-- CA92.1: Access user defaults for app-only configuration/state. -->
				<string>CA92.1</string>
			</array>
		</dict>

		<dict>
			<key>NSPrivacyAccessedAPIType</key>
			<string>NSPrivacyAccessedAPICategoryFileTimestamp</string>
			<key>NSPrivacyAccessedAPITypeReasons</key>
			<array>
				<!-- C617.1: Access file metadata (timestamps/size) for files inside the app container (e.g. local caches). -->
				<string>C617.1</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
